#include <stdio.h>
int main(){
    int r;
    printf("enter r : \n");
    scanf("%d",&r);
    int i;
    int j;
    int k;
    for (i=1;i<=r;i++){
        for (j=1;j<=r-i;j++){
            printf("-");
        }
        for (k=1;k<=i;k++){
            printf("*");
        }
        printf("\n");
    }
}
