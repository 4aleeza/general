#include <stdio.h>
int main(){
    int r,c;
    printf("enter r : \n");
    scanf("%d",&r);
    printf("enter c : \n");
    scanf("%d",&c);
    int arr[r][c];
    for (int i=0;i<r;i++){
        for (int j=0;j<c;j++){
            printf("arr[%d][%d] : \n",i,j);
            scanf("%d",&arr[i][j]);
        }
    }
    printf("ORIGINAL : \n");
    for (int i=0;i<r;i++){
        for (int j=0;j<c;j++){
            printf("%d ",arr[i][j]);
        }
        printf("\n");
    }
    printf("TRANSPOSED : \n");
     for (int i=0;i<c;i++){
        for (int j=0;j<r;j++){
            printf("%d ",arr[j][i]);
        }
        printf("\n");
    }

}
