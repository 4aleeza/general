//array of structs
#include <stdio.h>
#include <string.h>
int main(){
    typedef struct stud {
        int arn;
        char name[15];
        char grade;
    }stud;
    stud arr[2];
    arr[0].arn=1;
    strcpy(arr[0].name,"aleeza");
    arr[0].grade='A';
    arr[1].arn=2;
    strcpy(arr[1].name,"aliza");
    arr[1].grade='A';
    for (int i=0;i<2;i++){
        printf("NAME : %s ARN : %d GRADE : %c \n",arr[i].name,arr[i].arn,arr[i].grade);
    }
}
