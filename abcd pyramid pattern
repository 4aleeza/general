#include <stdio.h>
int main(){
    int n;
    int i;
    int j;
    int k;
    char a=65;
    printf("enter n : \n");
    scanf("%d",&n);
    for (i=1;i<=n;i++){
        for (j=1;j<=n-i;j++){
            printf(" ");
        }
        for (k=1;k<=i;k++){
            printf(" %c",a);
            a++;
        }
        printf("\n");
    }
}
